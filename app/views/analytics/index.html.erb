<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-2">
  </div>
</div>
<h1>Analytics/Reporting</h1>
<div class="ibox-content">
  <div class="panel-body">
    <div class="panel-group" id="accordion">
      <div class="panel panel-default">
        <div class="row">
          <div class="col-md-6">
            <%=render 'line_chart'%>
          </div>
          <div class="col-md-6">
            <%=render 'area_chart'%>
          </div>
        </div>
        <div class="row">
          <div class="col-md-6">
            <%=render 'bar_chart'%>
          </div>
          <div class="col-md-6">
            <%=render 'pie_chart'%>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%= link_to "Back", :back %>
