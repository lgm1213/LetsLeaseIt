    <div class="row">
      <div class="form-group">
        <div class="col-lg-2">
          <%= f.label :show_instruction, "Showing Instructions" %>
          <br>
          <br>
          <%= f.collection_select(:show_instruction_ids, ShowInstruction.all, :id, :options,{})%>
                     
        </div>
        <div class="col-lg-2">
          <%= f.label :compensation_trans_broker, "Comensation Trans Broker" %>
          <%= f.text_field :compensation_trans_broker, class: 'form-control' %>
        </div>
        <div class="col-lg-2">
          <%= f.label :compensation_buyers_agent, "Compensation Buyer's Agent" %>
          <%= f.text_field :compensation_buyers_agent, class: 'form-control' %>
        </div>
        <div class="col-lg-2">
          <%= f.text_field :compensation_non_rep, class: 'form-control' %>
          <%= f.label :compensation_non_rep, "Compensation Non Rep" %>
        </div>
        <div class="col-lg-2">
          <%= f.label :renewal_commission, "Renewal Commission" %>
          <%= f.select :renewal_commission, options_for_select(renewal_commision), {}, class: 'form-control' %>
        </div>
      </div>
    </div>
    <br>
    <br>
    <div class="row">
      <div class="col-lg-4">
        <%= f.label :agent_url, "Agent URL" %>
        <%= f.text_field :agent_url, class: 'form-control' %>
      </div>
      <div class="col-lg-4">
        <%= f.label :photo_instructions, "Photo Instructions" %>
        <%= f.text_field :photo_instructions, class: 'form-control' %>
      </div>
      <div class="col-lg-4">
        <%= f.label :company_id, "Company Rental Community Belongs To" %>
        <%= f.collection_select(:company_id, Company.all, :id, :name, prompt: true) %>
      </div>
    </div>  
  </fieldset>

  <h1>Images</h1>
  <fieldset>
    <h2>Images</h2>
    <div class="row">
      <div class="form-group">
        <div class="col-lg-4">
          <%= f.fields_for :building_images do |image|%>
            <%= render 'building_image_fields', f: image%>
          <%end%>
          <div class="links">
            <%= link_to_add_association 'Add image', f, :building_images, class: 'btn btn-success'%>
          </div>
        </div>
      </div>
    </div>
  </fieldset>

  <h1>Managers</h1>
  <fieldset>
    <h2>Assign Rental Community Managers</h2>
    <div class="row">
      <div class="form-group">
        <div class="col-lg-4">
          <%= f.label :account_manager %>
          <%= f.select :account_manager_id, User.where(role: 'account_manager').map{|u| [u.email, u.id]}, class: 'form-control' %>
        </div>
        <div class="col-lg-4">
          <%= f.label :regional_manager %>
          <%= f.select :regional_manager_id, User.where(role: 'regional_manager').map{|u| [u.email, u.id]}, class: 'form-control' %>
        </div>
        <div class="col-lg-4">
          <%= f.label :property_manager %>
          <%= f.select :property_manager_id, User.where(role: 'property_manager').map{|u| [u.email, u.id]}, class: 'form-control' %>
        </div>
      </div>
    </div>
  </fieldset>


  <div class="actions">
    <%#= f.submit %>
  </div>
<% end %>
